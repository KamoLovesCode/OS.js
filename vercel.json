{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**", "src/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server/index.js"
      }
    ]
  }
  